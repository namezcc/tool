		"name": "Launch test function",
		"type": "go",
		"request": "launch",
		"mode": "test",
		"program": "${workspaceFolder}/monitor/test",
		"args": [
			"-test.run",
			"Example"
		]
	},
	{
		"name": "Launch Main",
		"type": "go",
		"request": "launch",
		"mode": "debug",
		"program": "${workspaceFolder}/monitor/server/monitor/main.go",
		"cwd": "${workspaceFolder}/monitor/bin"
	},
	{
		"name": "Launch Master",
		"type": "go",
		"request": "launch",
		"mode": "debug",
		"program": "${workspaceFolder}/monitor/server/master/master.go",
		"cwd": "${workspaceFolder}/monitor/bin"
	},
		{
			"name": "Launch file",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${file}",
			"cwd": "${workspaceFolder}/monitor/bin"
		}
	]
}